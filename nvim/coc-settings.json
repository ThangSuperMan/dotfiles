{
  "languageserver": {
    "sorbet": {
      "command": "srb",
      "args": [
        "tc",
        "--typed",
        "true",
        "--enable-all-experimental-lsp-features",
        "--lsp",
        "--disable-watchman",
        "--dir",
        "."
      ],
      "filetypes": ["ruby"],
      "rootPatterns": ["sorbet/config"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  // Suggesstions
  "suggest.completionItemKindLabels": {
    "text": "",
    "method": "",
    "module": "",
    "interface": "",
    "function": "",
    "field": "ﰠ",
    "variable": "",
    "event": "",
    "class": "ﴯ",
    "file": "",
    "folder": "",
    "unit": "塞",
    "value": "",
    "keyword": "",
    "color": "",
    "snippet": "",
    "reference": "",
    "enum": "",
    "struct": "פּ",
    "constant": "",
    "operator": ""
  },

  // Setup coc-java
  // "java.referencesCodeLens.enabled": true,

  // Setup html
  // "html.filetypes": ["html"],
  // Codelens
  // "codeLens.enable": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "scss", "json"],

  "eslint.autoFixOnSave": true,
  "eslint.autoFix": true

  // "diagnostic.errorSign": "",
  // "diagnostic.warningSign": "",
  // "diagnostic.hintSign": "",
  // "diagnostic.infoSign": ""
}
