{
  "languageserver": {
    "sorbet": {
      "command": "srb",
      "args": [
        "tc",
        "--typed",
        "true",
        "--enable-all-experimental-lsp-features",
        "--lsp",
        "--disable-watchman",
        "--dir",
        "."
      ],
      "filetypes": ["ruby"],
      "rootPatterns": ["sorbet/config"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  // Suggesstions
  "suggest.completionItemKindLabels": {
    "text": "",
    "method": "",
    "module": "",
    "interface": "",
    "function": "",
    "field": "ﰠ",
    "variable": "",
    "event": "",
    "class": "ﴯ",
    "file": "",
    "folder": "",
    "unit": "塞",
    "value": "",
    "keyword": "",
    "color": "",
    "snippet": "",
    "reference": "",
    "enum": "",
    "struct": "פּ",
    "constant": "",
    "operator": ""
  },

  "coc.preferences.formatOnSaveFiletypes": ["json", "css", "markdown"],
  "eslint.autoFixOnSave": true,
  "eslint.autoFix": true
}
